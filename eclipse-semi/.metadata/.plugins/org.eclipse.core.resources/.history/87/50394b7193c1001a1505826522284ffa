		integrity="sha256-QWo7LDvxbWT2tbbQ97B53yJnYU3WhH/C8ycbRAkjPDc="
		crossorigin="anonymous"></script>
	<script type="text/javascript">
		$.ajax({
			method : "GET",
			url : "https://openapi.naver.com/v1/search/encyc.xml?query=%EC%A3%BC%EC%8B%9D&display=10&start=1&sort=sim",
			data : { query : "컴퓨터" },
			headers: {"X-Naver-Client-Id": "dc8f1q41qcRkkMrT0abk"},
			headers: {"X-Naver-Client-Secret": "mX6xlacNY0"}
		}).done(function(msg) {
			console.log(msg);
			$("#result").html(msg.documents[0].title+"<br>"+msg.documents[0].contents);
		});
	</script>
