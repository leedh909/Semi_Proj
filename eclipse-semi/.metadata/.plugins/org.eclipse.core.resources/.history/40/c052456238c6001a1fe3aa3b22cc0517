	$(function(){
		$("#choose1").hide();
		$("#choose2").hide();
		$("#choose3").hide();
	});
	function choose(num){
		
		if(num==1){
			$("#choose1").show();
			
		}else if(num==2){
			$("#choose2").show();
		}else if(num==3){
			$("#choose3").show();
		}
		
	}
	
	</script>
